# Ambiente
NODE_ENV=development
PORT=3000

# Banco de dados
DATABASE_URL="postgresql://user:password@localhost:5432/startup?schema=public"

# Redis
REDIS_URL="redis://localhost:6379"

# JWT
JWT_SECRET="seu-jwt-secret-aqui"
REFRESH_SECRET="seu-refresh-secret-aqui"

# APIs externas
STRIPE_SECRET_KEY="seu-stripe-secret-key-aqui"
OPENAI_API_KEY="sua-openai-api-key-aqui"
CLAUDE_API_KEY="sua-claude-api-key-aqui"
DEEPSEEK_API_KEY="sua-deepseek-api-key-aqui"

# Configurações de email
SMTP_HOST="smtp.exemplo.com"
SMTP_PORT=587
SMTP_USER="seu-email@exemplo.com"
SMTP_PASS="sua-senha-aqui"

# Configurações de upload
UPLOAD_DIR="uploads"
MAX_FILE_SIZE=5242880 # 5MB em bytes

# Configurações de cache
CACHE_TTL=3600 # 1 hora em segundos

# Configurações de rate limit
RATE_LIMIT_WINDOW=900000 # 15 minutos em milissegundos
RATE_LIMIT_MAX=100 # número máximo de requisições por janela

# Configurações de logging
LOG_LEVEL="info"
LOG_DIR="logs" 